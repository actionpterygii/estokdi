---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Team from '../components/Team.astro';
import Footer from '../components/Footer.astro';

const path = '/apex_legends';
const division_name = 'Apex Legends';
const m_c_division = 'm_c__apex_legends';
const title = `${division_name}プロチーム一覧`;
const descrption= `${division_name}のチームを持っている日本の著名プロeスポーツチームの参加ゲームタイトルと、Webサイト,ショップ,SNS,メンバー一覧,Liquipediaなどのリンクがまとめられています。`;
---
<Layout
	title={title}
	descrption={descrption}
	path={path}
	m_c_division={m_c_division}
>
	<Header title={title}></Header>
	<main>
		<Team></Team>
	</main>
	<Footer></Footer>
</Layout>

<style>
	main {
		padding-top: 20px;
		background-color: #FFF;
	}
</style>