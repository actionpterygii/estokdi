---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Team from '../components/Team.astro';
import Footer from '../components/Footer.astro';

const title = 'プロeスポーツチーム一覧';
const descrption='プロeスポーツチームの一覧です。Apex Legends, VALORANT, STREET FIGHTER, League of Legends などのゲームタイトルの部門を持っている日本の著名eスポーツチームの参加ゲームタイトルと、Webサイト,ショップ,SNS,メンバー一覧,Liquipediaなどのリンクがまとめられています。';
const path = '';
const m_c_division = '';
---
<Layout
	title={title}
	descrption={descrption}
	path={path}
	m_c_division={m_c_division}
>
	<Header title={title}></Header>
	<main>
		<Team></Team>
	</main>
	<Footer></Footer>
</Layout>

<style>
	main {
		padding-top: 20px;
		background-color: #FFF;
	}
</style>